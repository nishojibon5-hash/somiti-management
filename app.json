{
  "name": "SomitiManager",
  "displayName": "সমিতি ম্যানেজার",
  "expo": {
    "name": "সমিতি ম্যানেজার",
    "slug": "somiti-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.somitimanager.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.somitimanager.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "output": "static",
      "build": {
        "babel": {
          "include": [
            "client/**/*"
          ]
        }
      }
    }
  }
}