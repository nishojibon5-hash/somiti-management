{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "mobile:init": "npx cap init 'সমিতি ম্যানেজার' 'com.somitimanager.app'",
    "mobile:add": "npx cap add android",
    "mobile:copy": "npx cap copy android",
    "mobile:build": "npm run build:client && npx cap copy android",
    "mobile:open": "npx cap open android",
    "apk:debug": "cd android && ./gradlew assembleDebug",
    "apk:release": "cd android && ./gradlew assembleRelease",
    "web": "expo start --web",
    "web:build": "expo export:web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "native:build": "expo build",
    "cpanel:build": "expo export:web && npm run cpanel:prepare",
    "cpanel:prepare": "node scripts/prepare-cpanel.js"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "zod": "^3.23.8",
    "@capacitor/android": "^5.6.0",
    "@capacitor/cli": "^5.6.0",
    "@capacitor/core": "^5.6.0",
    "@capacitor/splash-screen": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "react-native-web": "^0.19.12",
    "@react-native-async-storage/async-storage": "^1.23.1"
  },
  "devDependencies": {
    "babel-preset-expo": "~11.0.14",
